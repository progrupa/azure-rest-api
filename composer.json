{
    "name": "progrupa/azure-rest-api",
    "description": "Auto-generated Microsoft Azure REST API library",
    "keywords": [
        "swagger",
        "php",
        "azure",
        "api"
    ],
    "homepage": "http://progrupa.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Dominik Kasprzak"
        },
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "~6.0",
        "sainsburys/guzzle-oauth2-plugin": "~3.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Progrupa\\Azure\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Progrupa\\Azure\\" : "test/" }
    }
}
